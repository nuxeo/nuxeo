      <name>Nuxeo virtual snapshot repository</name>                                                                                                        
      <releases>                                                                                                                                            
        <enabled>false</enabled>                                                                                                                            
      </releases>                                                                                                                                           
      <snapshots>                                                                                                                                           
        <enabled>true</enabled>                                                                                                                             
      </snapshots>                                                                                                                                          
    </pluginRepository>                                                                                                                                     
  </pluginRepositories> 

  
  
