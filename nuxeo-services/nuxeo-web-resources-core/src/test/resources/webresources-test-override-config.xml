<?xml version="1.0"?>
<component name="org.nuxeo.theme.styling.tests.themeContrib.override">

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">

    <resource name="my.css">
      <path>css/my.css</path>
    </resource>

  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">

    <bundle name="myapp">
      <resources append="true">
        <resource>my.css</resource>
      </resources>
    </bundle>

  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="processors">

    <processor name="myProc" enabled="false" />

  </extension>

</component>
