  <extension target="org.nuxeo.ecm.core.cache.CacheServiceImpl" point="caches">
    <cache name="default-test-cache" class="org.nuxeo.ecm.core.redis.RedisCacheImpl">
      <ttl>1</ttl>
    </cache>
  </extension>
</component>

