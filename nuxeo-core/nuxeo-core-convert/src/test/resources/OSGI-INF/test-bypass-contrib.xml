    <converter name="testBypassDefault" class="org.nuxeo.ecm.core.convert.tests.DummyConverter">
      <destinationMimeType>application/pdf</destinationMimeType>
      <sourceMimeType>application/pdf</sourceMimeType>
    </converter>

  </extension>

</component>
