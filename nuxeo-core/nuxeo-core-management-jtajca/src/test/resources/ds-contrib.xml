<?xml version="1.0"?>
<component name="org.nuxeo.runtime.jtajca.management.test.ds">
  <extension target="org.nuxeo.runtime.datasource" point="datasources">
    <datasource name="jdbc/repository_test" driverClassName="${nuxeo.test.vcs.driver}"
      maxActive="20" maxIdle="5">
      <property name="url">${nuxeo.test.vcs.url}</property>
      <property name="user">${nuxeo.test.vcs.user}</property>
      <property name="password">${nuxeo.test.vcs.password}</property>
    </datasource>
  </extension>
</component>