<?xml version="1.0"?>
<component name="org.nuxeo.ecm.core.versioning.test.override.contrib">

  <extension target="org.nuxeo.ecm.core.versioning.VersioningService" point="versioningRules">

    <versioningRule typeName="File" enabled="false" />

    <defaultVersioningRule>
      <initialState major="2" minor="2" />
      <options lifeCycleState="project">
        <none />
        <minor default="true" />
      </options>
      <options lifeCycleState="approved" />
    </defaultVersioningRule>
    
  </extension>
</component>
