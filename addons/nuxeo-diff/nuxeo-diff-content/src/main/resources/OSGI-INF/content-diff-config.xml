<?xml version="1.0"?>
<component name="org.nuxeo.ecm.diff.settings">
  <extension target="org.nuxeo.runtime.ConfigurationService" point="configuration">
    <documentation>
      Allows to limit the diff count between two files.
      @since 10.10-HF64
    </documentation>
    <property name="nuxeo.diff.limit">${nuxeo.diff.limit:=100000}</property>
  </extension>
</component>
