<?xml version="1.0"?>
<component name="org.nuxeo.ecm.platform.forms.layoutDemo.directories">

  <extension target="org.nuxeo.ecm.directory.sql.SQLDirectoryFactory"
    point="directories">

    <directory name="layout_demo_crew">
      <schema>vocabulary</schema>
      <dataSource>java:/nxsqldirectory</dataSource>
      <cacheTimeout>3600</cacheTimeout>
      <cacheMaxSize>1000</cacheMaxSize>
      <table>layout_demo_crew</table>
      <idField>id</idField>
      <autoincrementIdField>false</autoincrementIdField>
      <dataFile>directories/layout_demo_crew.csv</dataFile>
      <createTablePolicy>always</createTablePolicy>
    </directory>

    <directory name="l10ncoverage_small">
      <schema>l10nxvocabulary</schema>
      <dataSource>java:/nxsqldirectory</dataSource>
      <cacheTimeout>3600</cacheTimeout>
      <cacheMaxSize>1000</cacheMaxSize>
      <table>l</table>
      <idField>id</idField>
      <autoincrementIdField>false</autoincrementIdField>
      <dataFile>directories/l10ncoverage_small.csv</dataFile>
      <createTablePolicy>always</createTablePolicy>
    </directory>

  </extension>

</component>
