        <locale>gl_ES</locale><!-- Galician -->
        <locale>hu_HU</locale><!-- Hungarian - Hungary -->
        <locale>ja_JP</locale><!-- Japanese (Gregorian calendar) - Japan -->
        <locale>mk_MK</locale><!-- Macedonian - Macedonia -->
        <locale>pl</locale><!-- Polish - Poland -->
        <locale>pl_PL</locale><!-- Polish - Poland -->
        <locale>pt</locale><!-- Portuguese - Portugal -->
        <locale>pt_PT</locale><!-- Portuguese - Portugal -->
        <locale>pt_BR</locale><!-- Portuguese - Brazil -->
        <locale>ru</locale><!-- Russian - Russia -->
        <locale>ru_RU</locale><!-- Russian - Russia -->
        <locale>sr_SP</locale><!-- Serbian(Cyrillic) - Serbia -->
        <locale>tr_TR</locale><!-- Turkish - Turkey -->
        <locale>vi_VN</locale><!-- Vietnamese - Vietnam -->
