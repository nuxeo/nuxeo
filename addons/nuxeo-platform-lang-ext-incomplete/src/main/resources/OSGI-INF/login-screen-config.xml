      <supportedLocales append="true">
        <locale>ar_SA</locale><!-- Arabic - Saudi Arabia -->
        <locale>az_AZ</locale><!-- Azerbaijani - Azerbaijan -->
        <locale>ca_ES</locale><!-- Catalan - Spain -->
        <locale>cs_CZ</locale><!-- Czech - Czech Republic -->
        <locale>el_GR</locale><!-- Greek - Greece -->
