  </parent>

  <groupId>org.nuxeo.ecm.platform</groupId>
  <artifactId>nuxeo-platform-document-routing</artifactId>
  <name>Nuxeo Platform Document Routing Addon</name>
  <packaging>pom</packaging>
  <description>The parent module for Nuxeo Platform Document Routing Addon</description>

  <modules>
    <module>nuxeo-platform-document-routing-api</module>
    <module>nuxeo-platform-document-routing-core</module>
    <module>nuxeo-platform-document-routing-web</module>
    <module>nuxeo-platform-document-routing-dm</module>
    <module>nuxeo-platform-document-routing-dashboard</module>
    <module>nuxeo-platform-document-routing-default</module>
    <module>nuxeo-platform-document-routing-test</module>
  </modules>

  <developers>
    <developer>
      <name>Atlantis</name>
      <email>atlantis@lists.nuxeo.com</email>
    </developer>
