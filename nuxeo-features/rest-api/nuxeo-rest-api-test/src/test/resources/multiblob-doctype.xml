<component name="multiblob-doctype">


  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="schema">
    <schema name="multiblob" prefix="mb"
      src="multiblob.xsd" />
  </extension>

  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="doctype">

    <doctype name="MultiBlobDoc" extends="Document">
      <schema name="multiblob" />
      <schema name="dublincore" />
      <schema name="common" />
    </doctype>

  </extension>


</component>