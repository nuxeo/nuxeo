<?xml version="1.0"?>
<package type="addon" name="nuxeo-imap-connector" version="@VERSION@">
  <title>Nuxeo IMAP Connector</title>
  <description>
    The Nuxeo IMAP Connector addon allows to fetch emails from an IMAP mailbox and import them as Documents in the Nuxeo Repository.
  </description>
  <home-page>https://doc.nuxeo.com/userdoc/nuxeo-imap-connector/</home-page>
  <vendor>Nuxeo</vendor>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@VERSION@</platform>
    <platform>server-@VERSION@-*</platform>
  </platforms>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>PUBLIC</visibility>
</package>
