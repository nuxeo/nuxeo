<?xml version="1.0"?>
<package type="addon" name="nuxeo-datadog-reporter" version="@VERSION@">
  <title>Nuxeo Datadog reporter</title>
  <description>
    <p>The Nuxeo Datadog Reporter addon allows to report all the Nuxeo server metrics to Datadog, a SaaS platform to monitor your applications.</p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-datadog-reporter/</home-page>
  <vendor>Nuxeo</vendor>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@VERSION@</platform>
    <platform>server-@VERSION@-*</platform>
  </platforms>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>PUBLIC</visibility>
</package>
