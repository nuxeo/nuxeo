<?xml version="1.0"?>
<package type="addon" name="nuxeo-duoweb-authentication" version="@VERSION@">
  <title>Nuxeo DuoWeb Authentication</title>
  <description>
    <p>The Nuxeo DuoWeb Authentication addon is an integration of DuoWeb access in Nuxeo login plugin and provides
      two-factor authentication through the Nuxeo login page.
    </p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-duoweb-two-factor-authentication/</home-page>
  <vendor>Nuxeo</vendor>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@VERSION@</platform>
    <platform>server-@VERSION@-*</platform>
  </platforms>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>PUBLIC</visibility>
</package>
