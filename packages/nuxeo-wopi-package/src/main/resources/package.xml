<?xml version="1.0"?>
<package type="addon" name="nuxeo-wopi" version="@VERSION@">
  <title>Nuxeo WOPI</title>
  <description>
    <p>The Nuxeo WOPI addon adds the ability to view/edit Microsoft Office documents through Office Online.</p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-office-online-integration/</home-page>
  <vendor>Nuxeo</vendor>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@VERSION@</platform>
    <platform>server-@VERSION@-*</platform>
  </platforms>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>MARKETPLACE</visibility>
</package>
