## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##

# Default configuration values for templates
# Specific default values are defined in templates/{template_name}/nuxeo.defaults
nuxeo.data.dir=server/default/data/NXRuntime/data

# Default template to use if not defined in nuxeo.conf
nuxeo.templates=default

# Default IP address binding
nuxeo.bind.address=0.0.0.0

# publicly visible URL
nuxeo.url=http://localhost:8080/nuxeo
# internally visible URL (server-to-server)
#nuxeo.loopback.url=http://localhost:8080/nuxeo

# Application context path
org.nuxeo.ecm.contextPath=/nuxeo

org.nuxeo.ecm.product.name=Nuxeo DM

# Various other default values
nuxeo.db.host=
nuxeo.db.port=
nuxeo.db.name=
nuxeo.db.user=
nuxeo.db.password=
nuxeo.db.min-pool-size=
nuxeo.db.max-pool-size=20
nuxeo.vcs.min-pool-size=0
nuxeo.vcs.max-pool-size=20

nuxeo.notification.eMailSubjectPrefix=[Nuxeo]

# SMTP gateway server
mail.transport.host=localhost
# Mail server port
mail.transport.port=25
mail.transport.auth=false
mail.transport.user=anonymous
mail.transport.password=password
# The address mail will be from
mail.from=noreply@nuxeo.com

# Clustering
repository.clustering.enabled=false
repository.clustering.delay=1000
# If clustering is activated, set repository.binary.store=/path/to/some/shared/folder/for/binaries
repository.binary.store=

nuxeo.plaintext-parsing-extensions=xml,properties,nx

# JBoss configuration to run
org.nuxeo.ecm.jboss.configuration=default

server.status.key=9z3x8R73
nuxeo.config.jboss=server/default/deploy/nuxeo.ear/config
nuxeo.datasources.jboss=server/default/deploy/nuxeo.ear/datasources
nuxeo.config.tomcat=nxserver/config
nuxeo.datasources.tomcat=conf/Catalina/localhost/nuxeo.xml

test.nuxeo.defaults = true
test.nuxeo.defaults.template.1 = nuxeo.defaults
test.nuxeo.conf.override.defaults = nuxeo.defaults
