<?xml version="1.0"?>
<component name="org.nuxeo.ecm.liveconnect.dropbox.jsf.theme">

  <require>org.nuxeo.theme.nuxeo.webapp</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="nuxeo_includes">
      <resources append="true">
        <resource>dropboxclient.js</resource>
      </resources>
    </bundle>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="dropboxclient.js">
      <path>nxthemes/html/scripts/dropboxclient.js</path>
    </resource>
  </extension>

</component>
