{
  "name": "listing",
  "aliases": ["listingAlias"],
  "templates":   {
    "any": "/layouts/layout_listing_template.xhtml",
    "csv": "/layouts/layout_listing_csv_template.xhtml",
    "pdf": "/layouts/layout_listing_pdf_template.xhtml"
  },
  "configuration":   {
    "title": "Test layout type for a custom type category",
    "sinceVersion": "6.0",
    "defaultPropertyValues":     {
      "any":       {
        "showListingHeader": "true",
        "showRowEvenOddClass": "true"
      },
      "edit_columns":       {
        "availableElementsLabel": "label.selection.availableColumns",
        "columnSelectionRequired": "true",
        "displayAlwaysSelectedColumns": "false",
        "selectSize": "10",
        "selectedElementsHelp": "",
        "selectedElementsLabel": "label.selection.selectedColumns"
      },
      "edit_sort_infos":       {
        "newSortInfoTemplate": "#{documentSearchActions.newSortInfo}",
        "sortInfoSelectionRequired": "false"
      }
    }
  }
}