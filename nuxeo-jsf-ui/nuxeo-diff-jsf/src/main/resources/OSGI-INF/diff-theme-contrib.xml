<?xml version="1.0"?>
<component name="org.nuxeo.ecm.diff.theme">

  <require>org.nuxeo.theme.nuxeo.webapp</require>

  <!-- Theme pages -->
  <extension target="org.nuxeo.theme.styling.service" point="pages">

    <page name="galaxy/default">
      <resources append="true">
        <resource>diff.css</resource>
      </resources>
    </page>

  </extension>

  <!-- Styles -->
  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">

    <resource name="diff.css">
      <path>themes/css/diff_result.css</path>
    </resource>

  </extension>

</component>
