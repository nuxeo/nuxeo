## DO NOT EDIT THIS FILE, USE nuxeo.conf  ##
nuxeo.template.includes=common

nuxeo.server.http.port=8080
nuxeo.server.ajp.port=8009
nuxeo.server.https.port=8443
nuxeo.server.tomcat-admin.port=8005
nuxeo.server.jvmRoute=nuxeo

nuxeo.db.name=nuxeo
nuxeo.db.user=sa
nuxeo.db.password=
nuxeo.db.jdbc.url=jdbc:h2:${nuxeo.data.dir}/h2/${nuxeo.db.name};DB_CLOSE_ON_EXIT=false;MODE=LEGACY
nuxeo.db.driver=org.h2.Driver
nuxeo.db.xadatasource=org.h2.jdbcx.JdbcDataSource
nuxeo.db.commonds=jdbc/nuxeo

org.nuxeo.ecm.instance.description=Nuxeo ECM server

# Mail server protocol
mail.store.protocol=pop3
mail.transport.protocol=smtp
# Use TLS for SMTP connections
mail.transport.usetls=false
# User who will receive mail
mail.user=nobody
# Mail server
mail.store.host=pop3.nosuchhost.nosuchdomain.com
# Enable debugging output from the javamail classes
mail.debug=false

nuxeo.server.emptySessionPath=false
