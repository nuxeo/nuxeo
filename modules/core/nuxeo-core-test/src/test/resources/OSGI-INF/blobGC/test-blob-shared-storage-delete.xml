<?xml version="1.0"?>
<component name="org.nuxeo.ecm.core.blob.test-blob-delete" version="1.0.0">
  <extension target="org.nuxeo.ecm.core.blob.BlobManager" point="configuration">
    <blobprovider name="first">
      <class>org.nuxeo.ecm.core.blob.LocalBlobProvider</class>
      <property name="path">same</property>
    </blobprovider>
    <blobprovider name="second">
      <class>org.nuxeo.ecm.core.blob.LocalBlobProvider</class>
      <property name="path">same</property>
    </blobprovider>
  </extension>
  <extension target="org.nuxeo.ecm.core.blob.DocumentBlobManager" point="configuration">
    <blobdispatcher>
      <property name="default">first</property>
      <property name="dc:source=foo">second</property>
    </blobdispatcher>
  </extension>
</component>
